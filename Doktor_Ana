       public Doktor_Ana_Ekran(String sql) {
        initComponents();
      
        h_sql = sql ; 
        isim(sql);
        tarih();
        Toolkit toolkit=getToolkit();
        Dimension size=toolkit.getScreenSize();
        setLocation(size.width/2-getWidth()/2,size.height/2-getHeight()/2);
       
    
    }
       
       
        public Doktor_Ana_Ekran(String sql , int i) {
        initComponents();
        h_sql = sql ; 
            System.out.println("1");
        isim(sql);
        
            System.out.println("2"); 
        
        tarih();
            System.out.println("3");
            Toolkit toolkit=getToolkit();
        Dimension size=toolkit.getScreenSize();
        setLocation(size.width/2-getWidth()/2,size.height/2-getHeight()/2);
    }
       
    
    
    
     public void poliklinikbulma(){
        
        try{
            
            conn = db.connect_db();
            String sql = "SELECT * FROM POLIKLINIK WHERE POLIKLINIK_ID = '" + poliklinik_id + "'";
            pst  = conn.prepareStatement(sql);
            rs = pst.executeQuery() ; 
            
            while(rs.next()){
                txt_hb_poliklinik.setText(rs.getString("POLIKLINIK_AD"));
                txt_mhb_dpoliklinik.setText(rs.getString("POLIKLINIK_AD"));
                
                
                txt_mhb_dpoliklinik.setEnabled(false);
            }
            
           
            
        }catch (Exception ex) {
            JOptionPane.showMessageDialog(null, ex);
        }finally{
            try{
               conn.close();
            }catch (Exception ex) {
                
            }
        }
        
        
    }
    
    
     int d_id ;
     int poliklinik_id ; 
     public void isim(String sql){
         

         try{
             conn = db.connect_db(); 
            // String sql  = "SELECT * FROM HASTA WHERE HASTA_ID = '" + a + "'" ;
             pst = conn.prepareStatement(sql);
             rs = pst.executeQuery() ; 
             System.out.println(sql);
           
             while(rs.next()){
                 d_id = Integer.parseInt(rs.getString("DOKTOR_ID"));
                 txt_adsoyad.setText(rs.getString("DOKTOR_AD")  + " " + rs.getString("DOKTOR_SOYAD"));
                 
              txt_hb_ad.setText(rs.getString("DOKTOR_AD"));
              txt_hb_soyad.setText(rs.getString("DOKTOR_SOYAD"));
              txt_hb_telefon.setText(rs.getString("TELEFON_NO"));
             
              txt_hb_email.setText(rs.getString("E_MAIL"));
              poliklinik_id  = Integer.valueOf(rs.getString("POLIKLINIK_ID"));
              txt_hb_adres.setText(rs.getString("ADRES"));
              
             
             }
             
           
             
         }catch (Exception ex) {
             JOptionPane.showMessageDialog(null,ex);
         }finally{
             try{
                 conn.close();
             }catch (Exception ex) {
                 
             }
         }
         
              poliklinikbulma();
         
     }
     
     int gun ; 
     int ay ;
     int yıl ;
     String tarih; 
    
    public void tarih(){
        
    SimpleDateFormat dateFormat=new SimpleDateFormat("dd/MM/yyyy");
    
    Date guntarih= new Date();
    
    String deger = dateFormat.format(guntarih);
   
    
    String[] bol = deger.split("/");
    int[] cevir = new int[bol.length] ; 
        
        for (int i = 0; i < bol.length; i++) {
       
            cevir[i] = Integer.valueOf(bol[i]);
       
        }
        
            gun = cevir[0]; 
            ay = cevir[1];
            yıl = cevir[2];
            
            
       tarih = deger;

    }
    
   
     
     
      int aktifsutun = 0 ;
    int gecmissutun = 0 ; 
    
      void aktifrandevu(){
        
        try{
        
            DefaultTableModel tbl = (DefaultTableModel) tbl_aktifrandevu.getModel();
            
            while (tbl.getRowCount() > 0) {
                // eğer ki tabloda 1 satır bile veri varsa sorgusu yaptık
                tbl.removeRow(0);  // tüm satırları sil dedik 
            }
            
            //veritabanı işlemleri 
            
            conn = db.connect_db();
            String sql = "SELECT * FROM RANDEVU_ISLEM WHERE DOKTOR_ID = '" + d_id +  "'" ; 
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery();
            
            ResultSetMetaData rsmd = rs.getMetaData(); 
            aktifsutun = rsmd.getColumnCount(); 
            String[] data = new String[aktifsutun]; 
          
            while(rs.next()){
                System.out.println(rs.getString("HASTA_ID"));
              
                
                 for (int i = 0; i < data.length; i++) {
                  data[i] = rs.getString(i+1);

        
              }
           
                 DefaultTableModel tblModel = (DefaultTableModel) tbl_aktifrandevu.getModel(); 
                 tblModel.addRow(data); // verileri ekledik 
          
            }  
          
        }catch (Exception ex) {
            
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {
                
            }
        }
        
        
    }
      
      
      
       void gecmisrandevu(){
        System.out.println(h_id);
        try{

            DefaultTableModel tbl = (DefaultTableModel) tbl_gecmisrandevu.getModel();

            while (tbl.getRowCount() > 0) {
                // eğer ki tabloda 1 satır bile veri varsa sorgusu yaptık
                tbl.removeRow(0);  // tüm satırları sil dedik 
            }

            //veritabanı işlemleri 

            conn = db.connect_db();
            String sql = "SELECT * FROM GECMİS_RANDEVU WHERE DOKTOR_ID = '" + d_id +  "'" ; 
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery();
            
            

            ResultSetMetaData rsmd = rs.getMetaData(); // sütün sayısını alma işlemi 
            aktifsutun = rsmd.getColumnCount(); // sütun sayısı alındı 
            String[] data = new String[aktifsutun]; // sütun boyutunda bir String boyut oluşturduk

            while(rs.next()){

                
                
                
                for (int i = 0; i < data.length; i++) {
                  data[i] = rs.getString(i+1);


              }

                 DefaultTableModel tblModel = (DefaultTableModel) tbl_gecmisrandevu.getModel(); 
                 tblModel.addRow(data); // verileri ekledik 

            }

        }catch (Exception ex) {

        }finally{
            try{
                conn.close();
            }catch (Exception ex) {

            }
        }
        
        
    }
       
       
       
       
        int tanısutun = 0 ; 
        void tanıgecmis(){
        System.out.println(h_id);
        try{

            DefaultTableModel tbl = (DefaultTableModel) tbl_gecmistanı.getModel();

            while (tbl.getRowCount() > 0) {
                // eğer ki tabloda 1 satır bile veri varsa sorgusu yaptık
                tbl.removeRow(0);  // tüm satırları sil dedik 
            }

            //veritabanı işlemleri 

            conn = db.connect_db();
            String sql = "SELECT * FROM TANI_ISLEM WHERE DOKTOR_ID = '" + d_id +  "'" ; 
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery();
            
            

            ResultSetMetaData rsmd = rs.getMetaData(); // sütün sayısını alma işlemi 
            tanısutun = rsmd.getColumnCount(); // sütun sayısı alındı 
            String[] data = new String[tanısutun]; // sütun boyutunda bir String boyut oluşturduk

            while(rs.next()){

                
                
                
                for (int i = 0; i < data.length; i++) {
                  data[i] = rs.getString(i+1);


              }

                 DefaultTableModel tblModel = (DefaultTableModel) tbl_gecmistanı.getModel(); 
                 tblModel.addRow(data); // verileri ekledik 

            }

        }catch (Exception ex) {

        }finally{
            try{
                conn.close();
            }catch (Exception ex) {

            }
        }
        
        
    }
       
       
     
     
     
     
    void gecmis(){
        int a =0 ;
        int ınsert = 0 ; 
        System.out.println("yazııı");
        
        try{
            System.out.println("1.");
            conn = db.connect_db() ; 
            String sql = "SELECT * FROM RANDEVU_ISLEM WHERE DOKTOR_ID = '" + d_id + "' " ;
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery() ; 
            while(rs.next()){
                a++ ; 
            }
        }catch (Exception ex) {
            JOptionPane.showMessageDialog(null, ex +".");
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {
                
            }
        }
        
        String[] il = new String[a] ;
        String[] idd = new String[a] ;
        String[] ilce = new String[a] ;
        String[] hastane = new String[a] ;
        String[] poliklinik = new String[a] ;
        String[] r_tarih = new String[a] ;
        String[] doktor_idd = new String[a] ;
        String[] hasta_idd = new String[a] ;
        String[] r_saatt = new String[a] ;
        boolean [] dogrulama = new boolean[a];
        
         try{
             System.out.println("2.");
            conn = db.connect_db() ; 
            String sql = "SELECT * FROM RANDEVU_ISLEM WHERE DOKTOR_ID = '" + d_id + "' " ;
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery() ; 
     
            int i = 0 ;
            while(rs.next()){
                 SimpleDateFormat dateFormat=new SimpleDateFormat("dd/MM/yyyy");
                 Date now= new Date();
               
                   
                    dogrulama[i] = false ;
                    idd[i] = rs.getString("RANDEVU_ID");
                    il[i] = rs.getString("RANDEVU_IL");
                    ilce[i] = rs.getString("RANDEVU_ILCE");
                    hastane[i] = rs.getString("HASTANE_AD");
                    poliklinik[i] = rs.getString("RANDEVU_POLIKLINIK");
                    r_tarih[i] = rs.getString("RANDEVU_TARIH");
                    doktor_idd[i] = rs.getString("DOKTOR_ID");
                    hasta_idd[i] = rs.getString("HASTA_ID");
                    r_saatt[i] = rs.getString("RANDEVU_SAAT");  
                   
                    Date date = dateFormat.parse(r_tarih[i]);
            
                    
                    if (now.getTime() > date.getTime()) 
                    {
                        dogrulama[i] = true ; 
                        
                        if (now.getYear() == date.getYear()) {
                            if (now.getMonth() == date.getMonth()) {
                                if (now.getDay() == date.getDay()) {
                                      dogrulama[i] = false ;
                                }
                            }
                        }
                        
                    }
                    
                     i++;  
                    
            }
        }catch (Exception ex) {
            JOptionPane.showMessageDialog(null, ex +".");
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {
                
            }
        }
         
         
        
        for (int i = 0; i < a; i++) {
            if (dogrulama[i] == true) {
                
                  try{
                      System.out.println("a : " + i );
                conn=db.connect_db();
                String sql = "INSERT INTO GECMİS_RANDEVU (GECMİS_IL,GECMİS_ILCE,HASTANE_AD,GECMİS_POLIKLINIK,GECMİS_TARIH,DOKTOR_ID,HASTA_ID,GECMİS_SAAT) VALUES (?,?,?,?,?,?,?,?)" ;
                pst = conn.prepareStatement(sql);
                pst.setString(1, il[i]);
                pst.setString(2, ilce[i]);
                pst.setString(3, hastane[i]);
                pst.setString(4, poliklinik[i]);
                pst.setString(5, r_tarih[i]);
                pst.setString(6, doktor_idd[i]);
                pst.setString(7, hasta_idd[i]);
                pst.setString(8, r_saatt[i]);
                pst.execute();
                gecmisrandevu();
            }
            catch (Exception ex) {
                   JOptionPane.showMessageDialog(null,ex );
            }
            finally{
                try{
                    conn.close();
                }catch (Exception ex) {

                }
            }
                
             }
            
            else{
                continue ; 
            }
            
        }
        
        for (int i = 0; i < a; i++) {
            
            if (dogrulama[i] == true) {
                
                  try{
                conn=db.connect_db();
                String sql = "DELETE FROM RANDEVU_ISLEM WHERE RANDEVU_ID=?" ;
                pst = conn.prepareStatement(sql);
                pst.setString(1, idd[i]);
                pst.execute();
                aktifrandevu();
              
            }
            catch (Exception ex) {
                JOptionPane.showMessageDialog(null,ex );
            }
            finally{
                try{
                    conn.close();
                }
                catch (Exception ex) {

                }
            }
                
            }
            
        }
        
        
        
       
        
    }
      
      
    
    
    
    
      int onaylanansutun;
      void onaylanantest(){
        
        try{
        
            DefaultTableModel tbl = (DefaultTableModel) tbl_labonaylanantest.getModel();
            
            while (tbl.getRowCount() > 0) {
                // eğer ki tabloda 1 satır bile veri varsa sorgusu yaptık
                tbl.removeRow(0);  // tüm satırları sil dedik 
            }
            
            //veritabanı işlemleri 
            
            conn = db.connect_db();
            String sql = "SELECT * FROM LAB_SONUC WHERE DOKTOR_ID = '" + d_id +  "'" ; 
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery();
            
            ResultSetMetaData rsmd = rs.getMetaData(); 
             onaylanansutun = rsmd.getColumnCount(); 
            String[] data = new String[onaylanansutun]; 
          
            while(rs.next()){
              
                
                 for (int i = 0; i < data.length; i++) {
                  data[i] = rs.getString(i+1);

        
              }
           
                 DefaultTableModel tblModel = (DefaultTableModel) tbl_labonaylanantest.getModel(); 
                 tblModel.addRow(data); // verileri ekledik 
          
            }  
          
        }catch (Exception ex) {
            
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {
                
            }
        }
        
        
    }
      
      
      
      
      
      
        int beklenilensutun = 0 ; 
        void beklenilentest(){
        
        try{
        
            DefaultTableModel tbl = (DefaultTableModel) tbl_labbeklenilentest.getModel();
            
            while (tbl.getRowCount() > 0) {
                // eğer ki tabloda 1 satır bile veri varsa sorgusu yaptık
                tbl.removeRow(0);  // tüm satırları sil dedik 
            }
            
            //veritabanı işlemleri 
            
            conn = db.connect_db();
            String sql = "SELECT * FROM LAB_GIRIS WHERE DOKTOR_ID = '" + d_id +  "'" ; 
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery();
            
            ResultSetMetaData rsmd = rs.getMetaData(); 
            beklenilensutun = rsmd.getColumnCount(); 
            String[] data = new String[beklenilensutun]; 
          
            while(rs.next()){
                System.out.println(rs.getString("HASTA_ID"));
              
                
                 for (int i = 0; i < data.length; i++) {
                  data[i] = rs.getString(i+1);

        
              }
           
                 DefaultTableModel tblModel = (DefaultTableModel) tbl_labbeklenilentest.getModel(); 
                 tblModel.addRow(data); // verileri ekledik 
          
            }  
          
        }catch (Exception ex) {
            
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {
                
            }
        }
        
        
    }
    
    
    
    
      
      

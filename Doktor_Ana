     public void muayenetestkilit(){
        cb_test.setEnabled(false);
        btn_testekle.setEnabled(false);
        btn_bitti.setEnabled(false);
        txt_test.setEnabled(false);
    }
    
    public void muayenetestkilitac(){
        cb_test.setEnabled(true);
        btn_testekle.setEnabled(true);
        txt_test.setEnabled(true);
    }
    
    
    public void muayenetanıkilit(){
        txt_tanı.setEnabled(false);
        txt_recete.setEnabled(false);
    }
    
    public void muayenetanıkilitac(){
        txt_tanı.setEnabled(true);
        txt_recete.setEnabled(true);
    }
    
    
    
    private void btn_testisteActionPerformed(java.awt.event.ActionEvent evt) {                                             

        muayenetestkilitac();
        muayenetanıkilit();
    }                                            


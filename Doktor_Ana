    private void btn_bittiActionPerformed(java.awt.event.ActionEvent evt) {                                          

         try{
             
             conn = db.connect_db() ; 
             String sql = " INSERT INTO LAB_GIRIS (LAB_IL , LAB_ILCE, HASTANE_AD,LAB_POLIKLINIK , LAB_TARIH,DOKTOR_ID , HASTA_ID , LAB_SAAT , LAB_TEST) VALUES (?,?,?,?,?,?,?,?,?)" ;
             pst = conn.prepareStatement(sql);
           
             pst.setString(1, doktor_il);
             pst.setString(2, doktor_ilce);
             pst.setString(3, doktor_hastane);
             pst.setString(4, doktor_poliklinik);
             pst.setString(5, doktor_tarih);
             pst.setString(6, doktor_id);
             pst.setString(7, hasta_id);
             pst.setString(8, doktor_saat);
             pst.setString(9, txt_test.getText());
             
             pst.execute();

             JOptionPane.showMessageDialog(null, "TEST İŞLEMİNİZ LABORATUVARA GÖNDERİLDİ");
             
         }catch (Exception ex) {
            JOptionPane.showMessageDialog(null, ex);
        }finally{
             try{
                 conn.close();
             }catch (Exception ex) {
                 
             }
         }
         
         
         try{
                conn=db.connect_db();
                String sql = "INSERT INTO GECMİS_RANDEVU (GECMİS_IL,GECMİS_ILCE,HASTANE_AD,GECMİS_POLIKLINIK,GECMİS_TARIH,DOKTOR_ID,HASTA_ID,GECMİS_SAAT) VALUES (?,?,?,?,?,?,?,?)" ;
                pst = conn.prepareStatement(sql);
              pst.setString(1, doktor_il);
             pst.setString(2, doktor_ilce);
             pst.setString(3, doktor_hastane);
             pst.setString(4, doktor_poliklinik);
             pst.setString(5, doktor_tarih);
             pst.setString(6, doktor_id);
             pst.setString(7, hasta_id);
             pst.setString(8, doktor_saat);
                pst.execute();
                gecmisrandevu();
            }
            catch (Exception ex) {
                   JOptionPane.showMessageDialog(null,ex );
            }
            finally{
                try{
                    conn.close();
                }catch (Exception ex) {

                }
            }
         
         
         try{
             
             conn = db.connect_db(); 
             String sql = "DELETE FROM RANDEVU_ISLEM WHERE RANDEVU_ID = ? "; 
             pst = conn.prepareStatement(sql); 
             
             pst.setString(1,randevu_id);
             
             pst.execute() ; 
             muayene_aktifrandevu();
             
             
         }catch (Exception ex) {
            JOptionPane.showMessageDialog(null, ex);
        }finally{
             try{
                 conn.close();
             }catch (Exception ex) {
                 
             }
         }
         
         
         
         
         
         
         tb_doktor.setSelectedIndex(0);
        
        
        
        

    }                                         

    String lab_id;
    String lab_il;
    String lab_ilce;
    String lab_hastane;
    String lab_poliklinik;
    String lab_tarih;
    String lab_doktor_id;
    String lab_hasta_id;
    String lab_saat;
    String lab_test;
    String lab_testsonuc;
    
    
    private void tbl_labonaylanantestMouseClicked(java.awt.event.MouseEvent evt) {                                                  

          String data []=new String [onaylanansutun];
        for (int i = 0; i <onaylanansutun ; i++) {
            data [i]=tbl_labonaylanantest.getValueAt(tbl_labonaylanantest.getSelectedRow(),i ).toString();
            System.out.println(data[i]);
        }
        
        lab_id = data[0];
        lab_il = data[1];
        lab_ilce = data[2];
        lab_hastane = data[3];
        lab_poliklinik= data[4];
        lab_tarih = data[5];
        lab_doktor_id = data[6];
        lab_hasta_id = data[7];
        lab_saat = data[8];
        lab_test = data[9] ; 
        lab_testsonuc = data[10];
        
        
                txt_lab_hasta.setText(data[7]);
        
        
        
    }                                                 

    private void btn_lab_sonucActionPerformed(java.awt.event.ActionEvent evt) {                                              
        
        int h_idd = Integer.valueOf(lab_hasta_id);
        int d_idd = Integer.valueOf(lab_doktor_id);
        
        if (h_idd!=0 && d_idd!=0) {
        labtanı = true ; 
        btn_testiste.setEnabled(false);
        btn_tanıkoy.setEnabled(false);
        muayenetestkilit();
        muayenetanıkilitac();
        hastabilgi(Integer.valueOf(lab_hasta_id));
        doktorbilgi(Integer.valueOf(lab_doktor_id));
        
        tb_doktor.setSelectedIndex(5);
            
        }
        
        else{
            JOptionPane.showMessageDialog(null, "Lütfen bir hasta seçiniz");
        }
        
    }                                             


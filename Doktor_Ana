    String randevu_id;
    String doktor_il;
    String doktor_ilce;
    String doktor_hastane;
    String doktor_poliklinik;
    String doktor_tarih;
    String hasta_id;
    String doktor_id;
    String doktor_saat;
    private void tbl_muayene_aktifrandevuMouseClicked(java.awt.event.MouseEvent evt) {                                                      
        
          String data []=new String [aktifsutun];
        for (int i = 0; i <aktifsutun ; i++) {
            data [i]=tbl_muayene_aktifrandevu.getValueAt(tbl_muayene_aktifrandevu.getSelectedRow(),i ).toString();
            System.out.println(data[i]);
        }
        
        randevu_id = data[0];
        doktor_il = data[1];
        doktor_ilce = data[2];
        doktor_hastane = data[3];
        doktor_poliklinik= data[4];
        doktor_tarih = data[5];
        doktor_id = data[6];
        hasta_id = data[7];
        doktor_saat = data[8];
        
                txt_muayeneid.setText(data[7]);
         
       
        
    }                                                     

    
     public void hastabilgi(int id){
        try{
            
            conn = db.connect_db()  ;
            String sql = "SELECT * FROM HASTA WHERE HASTA_ID = '" + id +  "'";
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery() ;
            
            while(rs.next()){
                
                txt_mhb_had.setText(rs.getString("HASTA_AD"));
                txt_mhb_hsoyad.setText(rs.getString("HASTA_SOYAD"));
                txt_mhb_htc.setText(rs.getString("TC"));
                txt_mhb_hdogumtarihi.setText(rs.getString("HASTA_DOGUMTARIHI"));
                txt_mhb_hcinsiyet.setText(rs.getString("CINSIYET"));
                
                txt_mhb_had.setEnabled(false);
                txt_mhb_hsoyad.setEnabled(false);
                txt_mhb_htc.setEnabled(false);
                txt_mhb_hdogumtarihi.setEnabled(false);
                txt_mhb_hcinsiyet.setEnabled(false);
                
                
            }
            
            
        }catch (Exception ex) {
            JOptionPane.showMessageDialog(null, ex);
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {
                
            }
        }
    }
    
    public void doktorbilgi(int id){
        try{
            
            conn = db.connect_db() ; 
            String sql = "SELECT * FROM DOKTOR WHERE DOKTOR_ID = ' "+ id + "'" ;
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery() ; 
            
            while(rs.next()){
                txt_mhb_dad.setText(rs.getString("DOKTOR_AD"));
                txt_mhb_dsoyad.setText(rs.getString("DOKTOR_SOYAD"));
                
                
                txt_mhb_dad.setEnabled(false);
                txt_mhb_dsoyad.setEnabled(false);
                
            }
            
        }catch (Exception ex) {
            JOptionPane.showMessageDialog(null, ex);
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {
                
            }
        }
                poliklinikbulma();
    }
    
    
    

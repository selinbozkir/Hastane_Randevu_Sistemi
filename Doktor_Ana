    void tanı(){
        
        if (tanı==true) {
         
             try{
           conn = db.connect_db()  ;
           String sql = "INSERT INTO TANI_ISLEM (TANI_IL,TANI_ILCE, HASTANE_AD , TANI_POLIKLINIK , TANI_TARIH , DOKTOR_ID , HASTA_ID , TANI_SAAT , TANI_TEST , TANI_TESTSONUC , TANI_TANI , TANI_RECETE) VALUES (?,?,?,?,?,?,?,?,?,?,?,?)  " ; 
           pst = conn.prepareStatement(sql);
           
           
           
           pst.setString(1,doktor_il);
           pst.setString(2,doktor_ilce);
           pst.setString(3,doktor_hastane);
           pst.setString(4,doktor_poliklinik);
           pst.setString(5,doktor_tarih);
           pst.setString(6,doktor_id);
           pst.setString(7,hasta_id);
           pst.setString(8,doktor_saat);
           pst.setString(9,"Test yapılmadı");
           pst.setString(10,"Test Yapılmadı");
           pst.setString(11,txt_tanı.getText());
           pst.setString(12,txt_recete.getText());
           pst.execute();
           JOptionPane.showMessageDialog(null, "Muayene Edildi");
           
       }catch (Exception ex) {
       
           JOptionPane.showMessageDialog(null, ex);
       
       }finally{
           try{
               conn.close();
           }catch (Exception ex) {
               
           }
       }
             
              try{
                conn=db.connect_db();
                String sql = "INSERT INTO GECMİS_RANDEVU (GECMİS_IL,GECMİS_ILCE,HASTANE_AD,GECMİS_POLIKLINIK,GECMİS_TARIH,DOKTOR_ID,HASTA_ID,GECMİS_SAAT) VALUES (?,?,?,?,?,?,?,?)" ;
                pst = conn.prepareStatement(sql);
              pst.setString(1, doktor_il);
             pst.setString(2, doktor_ilce);
             pst.setString(3, doktor_hastane);
             pst.setString(4, doktor_poliklinik);
             pst.setString(5, doktor_tarih);
             pst.setString(6, doktor_id);
             pst.setString(7, hasta_id);
             pst.setString(8, doktor_saat);
                pst.execute();
                gecmisrandevu();
            }
            catch (Exception ex) {
                   JOptionPane.showMessageDialog(null,ex );
            }
            finally{
                try{
                    conn.close();
                }catch (Exception ex) {

                }
            }
             
        }
        
        
        
        else if(labtanı==true){
            
              try{
           conn = db.connect_db()  ;
           String sql = "INSERT INTO TANI_ISLEM (TANI_IL,TANI_ILCE, HASTANE_AD , TANI_POLIKLINIK , TANI_TARIH , DOKTOR_ID , HASTA_ID , TANI_SAAT , TANI_TEST , TANI_TESTSONUC , TANI_TANI , TANI_RECETE) VALUES (?,?,?,?,?,?,?,?,?,?,?,?)  " ; 
           pst = conn.prepareStatement(sql);
           
           
           
           pst.setString(1,lab_il);
           pst.setString(2,lab_ilce);
           pst.setString(3,lab_hastane);
           pst.setString(4,lab_poliklinik);
           pst.setString(5,lab_tarih);
           pst.setString(6,lab_doktor_id);
           pst.setString(7,lab_hasta_id);
           pst.setString(8,lab_saat);
           pst.setString(9,lab_test);
           pst.setString(10,lab_testsonuc);
           pst.setString(11,txt_tanı.getText());
           pst.setString(12,txt_recete.getText());
           pst.execute();
           JOptionPane.showMessageDialog(null, "Muayene Edildi");
           
       }catch (Exception ex) {
       
           JOptionPane.showMessageDialog(null, ex);
       
       }finally{
           try{
               conn.close();
           }catch (Exception ex) {
               
           }
       } 
            
              
              try{
                  
                  conn = db.connect_db() ;
                  String sql = "INSERT INTO LAB_SONUCTARIHLERI (LAB_IL,LAB_ILCE, HASTANE_AD , LAB_POLIKLINIK , LAB_TARIH , DOKTOR_ID , HASTA_ID , LAB_SAAT , LAB_TEST , LAB_TESTSONUC ) VALUES (?,?,?,?,?,?,?,?,?,?) " ; 

    void botyenile (javax.swing.JTextField txtbotsorgula,javax.swing.JTextField txtbotsayı){
            Random r=new Random();
        int sayı=r.nextInt(9000)+999;
        String bot=Integer.toString(sayı);
        txtbotsorgula.setText(bot);
        txtbotsorgula.setEnabled(false);
        txtbotsayı.setText("");
    }
    
    

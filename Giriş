     void giris (javax.swing.JTextField txt,javax.swing.JTextField txtsifre, String x,javax.swing.JLabel lbl,javax.swing.JTextField txtbot,javax.swing.JTextField txtbotsayı){
       if (x.equals("HASTA")) {
           try {
               conn=db.connect_db();
               String sql =  "SELECT * FROM "+ x +" WHERE TC = '" + txt.getText() +  "' AND PAROLA = '" + txtsifre.getText() + "'";
               pst=conn.prepareStatement(sql);
               rs=pst.executeQuery(); 
             int b = 0;           
                for (int i = 0; i <txt.getText().length(); i++) {
                     b++;  
                   }
                      if (b==11) {
                         if (rs.next()) {
                            if (txtbotsayı.getText().equals(txtbot.getText())) {
                  
                                lbl.setText("");
                                JOptionPane.showMessageDialog(null,"HOŞ GELDİNİZ");
                                int id=Integer.parseInt(rs.getString("HASTA_ID"));
                                new Ana_Ekran("SELECT * FROM HASTA WHERE HASTA_ID='"+id+"'").setVisible(true);              
                                this.hide();
                                 }
                            
                            else{
                                     
                                 JOptionPane.showMessageDialog(null,"Lütfen Güvenlik Kodunu Doğru ve Tam Giriniz");
                                 botyenile (txtbot,txtbotsayı);
                                 txt.setText("");
                                 txtsifre.setText("");
                              }

                             }
                          else{
                              txt.setText("");
                              txtsifre.setText("");
                              lbl.setText("TC kimlik no veya şifre yanlış yeniden giriş yapınız");
                              botyenile (txtbot,txtbotsayı);
                             }
                         }
               else {
                     System.out.println(b);
                     JOptionPane.showMessageDialog(null, "11 rakamlı TC kimlik no giriniz!");
                     botyenile (txtbot,txtbotsayı);
                     txt.setText("");
                     txtsifre.setText("");
                        
              }
           } 
           catch (Exception ex) {
                JOptionPane.showMessageDialog(null, ex);
            }
             finally {
              try {
                conn.close();
            }
              catch (Exception ex) {

            }
        }
           
        }
        else if (x.equals("DOKTOR")) {
            try{
                conn=db.connect_db();
                String sql =  "SELECT * FROM "+ x +" WHERE KULLANICI_AD = '" + txt.getText() +  "' AND PAROLA = '" + txtsifre.getText() + "'";
                pst=conn.prepareStatement(sql);
                rs=pst.executeQuery(); 
          if (rs.next()){
                
              if (txtbotsayı.getText().equals(txtbot.getText())) {
                  
                    lbl.setText("");
                    JOptionPane.showMessageDialog(null,"HOŞ GELDİNİZ");
                    int d_id=Integer.parseInt(rs.getString("Doktor_ID"));
                    new Doktor_Ana_Ekran ("SELECT * FROM DOKTOR WHERE DOKTOR_ID='"+d_id+"'").setVisible(true);
                    this.hide();
                }
                
                
                else {
                    JOptionPane.showMessageDialog(null,"Lütfen Güvenlik Kodunu Doğru ve Tam Giriniz");
                    botyenile (txtbot,txtbotsayı);
                    txt.setText("");
                    txtsifre.setText("");
              }
          }
         else{
                   
                     txt.setText("");
                     txtsifre.setText("");
                     lbl.setText("Kullanıcı adı veya şifre yanlış yeniden giriş yapınız");
                     botyenile (txtbot,txtbotsayı);
                }
            }
            catch (Exception ex) {
                
            }
              finally {
               try {
                conn.close();
            }
               catch (Exception ex) {

            }
        }
        }
       
        else if(x.equals("LAB")){
            
              try{
            
            conn = db.connect_db(); 
            String sql =  "SELECT * FROM  LAB WHERE LAB_KULLANICIAD = '" + txt_labkulad.getText() +  "' AND LAB_PAROLA = '" + txt_labparola.getText() + "'";
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery();
            
            if (rs.next()) {
                
                if (txt_lab_botsorgula.getText().equals(txt_lab_botsayı.getText())) {
                lbl_labhata.setText("");
                JOptionPane.showMessageDialog(null, "Hoş Geldiniz");
                int id = Integer.parseInt(rs.getString("LAB_ID"));
                    //new Hasta_Giris("SELECT * FROM HASTA WHERE HASTA_ID = '"+ id +"'").setVisible(true);
                    new Lab_Ana_Ekran("SELECT * FROM LAB WHERE LAB_ID = '"+ id +"'").setVisible(true);
                    this.hide();
                 
                }
                
                else{
                    JOptionPane.showMessageDialog(null, "Lütfen güvenlik kodunu doğru ve tam giriniz");
                }
            }
           
            else{
                    //JOptionPane.showMessageDialog(null,"hata");
                     txt_labkulad.setText("");
                     txt_labparola.setText("");
                     lbl_labhata.setText("Kullanıcı adı veya Şifre yanlış yeniden giriş yapınız");
                    
                }

        }catch (Exception ex) {
              JOptionPane.showMessageDialog(null, ex);  
        }
         finally{
            try{
                conn.close();
            }catch (Exception ex) {
               
            }
        }
            
            
        }
        
       
     }
    
   
    
    private void ch_dsifreActionPerformed(java.awt.event.ActionEvent evt) {                                          

        try{
            if (ch_dsifre.isSelected()) {
                txt_dparola.setEchoChar((char)0);
            }else{
                txt_dparola.setEchoChar('*');
            }
        }catch (Exception ex) {
            
        }
        
    }                                         


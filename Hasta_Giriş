     int gun ; 
     int ay ;
     int yıl ;
    
     
    public void tarih(){
   
    SimpleDateFormat dateFormat=new SimpleDateFormat("dd/MM/yyyy");
    SimpleDateFormat dateFormat1=new SimpleDateFormat("dd/MM/yyyy");
    Date guntarih= new Date();
    //Date secilentarih = date.getDate();
    
    String deger = dateFormat1.format(guntarih);
     lbl_zaman.setText(deger);
   
//    SimpleDateFormat bicim=new SimpleDateFormat("dd/MM/yyyy");
//    Date tarih=new Date();
//    String zaman = bicim.format(tarih);
//String[] aylar={"Ocak","Şubat","Mart","Nisan","Mayıs","Haziran","Temmuz","Ağustos","Eylül","Ekim","Kasım","Aralık"};
//
//
//Calendar takvim=Calendar.getInstance();
//System.out.println("YIL    : " + takvim.get(Calendar.YEAR));
//System.out.println("AY     : " + takvim.get(Calendar.MONTH));
//System.out.println("GÜN    : " + takvim.get(Calendar.DATE));
   
    
    String[] bol = deger.split("/");
    int[] cevir = new int[bol.length] ; 
        
        for (int i = 0; i < bol.length; i++) {
       
            cevir[i] = Integer.valueOf(bol[i]);
       
        }
        
            gun = cevir[0]; 
            ay = cevir[1];
            yıl = cevir[2];
            
//            System.out.println("randevu tablosu açıldı : " + gun + "." + ay + "." + yıl);
//            
//        System.out.println("toplama : " + (cevir[0] + cevir[1]));
//
//System.out.println(bicim.format(tarih));

    }
    
    
    
    
    
    String h_tarih   ;
    public void zaman(String gun , String ay , String yıl){
            System.out.println("zaman hata ");
            h_tarih =  gun + "/" + ay + "/" + yıl ;
            
            int x=JOptionPane.showConfirmDialog(null, "Randevuyu Onaylıyor musunuz?","",JOptionPane.YES_NO_OPTION);
            
            if (x==0) {
            randevukayıt();  
            
            }
            else{
                JOptionPane.showMessageDialog(null,"Randevunuz Onaylanmadı");
            }
            
    }
    
    
    
    
    
    
    
    
    public void tarihsorgula(javax.swing.JComboBox<String> rgun , javax.swing.JComboBox<String> ray,javax.swing.JComboBox<String> ryıl){
//        String gunnn = (String) rgun.getSelectedItem();
//        String ayyy =  (String) ray.getSelectedItem();
//        String yılll = (String) ryıl.getSelectedItem();
        if (cb_randevugun.getSelectedIndex() != 0 && cb_randevuay.getSelectedIndex() != 0 && cb_randevuyıl.getSelectedIndex()!=0) {
            
        String gunnn = (String) rgun.getSelectedItem();
        String ayyy =  (String) ray.getSelectedItem();
        String yılll = (String) ryıl.getSelectedItem();
        
        // int değerlere çevirme işlemleri 
        int gunn = Integer.valueOf(gunnn);
        int ayy = Integer.valueOf(ayyy);
        int yıll = Integer.valueOf(yılll);
        
        System.out.println("tarih sorgulama : " + gunnn + "." + ayyy+ "." + yılll);
        System.out.println("int tarih sorgulama : " + gunn + "." + ayy+ "." + yıll);
        
        // randevu panelinde bir tarih yazdırdım o tarihteki String bugününtarih_gun,bugününtarih_ay ve bugününtarih_yıl değerleri int değer olarak atadım. sonra boş bir alana yazdığım int(gün ay ve yıl) değerlerine şuan ki tarihi gün ay ve yılını atadım
        
        // burada yapacağım sorgu bizim randevu saati seçeceğimiz zamanın  şu anki zamandan ilerdemi onu yapacağız (geçmiş bir tarihe randevu alamayız)
        if (yıll==yıl) {
                   
            
            if (ayy>ay) {
                   zaman(gunnn,ayyy,yılll);
                   
                
            }
            else if (ayy==ay) {
                
                if (gunn>gun) {
                   zaman(gunnn,ayyy,yılll);
                }
                else{
                    JOptionPane.showMessageDialog(null, "Lütfen geçerli bir tarih giriniz");
                }
                
            }
            
            else{
                JOptionPane.showMessageDialog(null, "Lütfen geçerli bir tarih giriniz");
            }
            
        }
        
        else if (yıll>yıl) {
            //İSTENİLEN TARİHE Randevu alabiliriz
                   zaman(gunnn,ayyy,yılll);
        }
        else{
            JOptionPane.showMessageDialog(null, "Lütfen geçerli bir tarih giriniz");
        }
        
            
        }
        else{
            JOptionPane.showMessageDialog(null,"Lütfen tarih giriniz");
        }

    }
    
    
    public void cb_kilitleme(){
        cb_ilce.setEnabled(false);
        cb_hastane.setEnabled(false);
        cb_poliklinik.setEnabled(false);
        cb_hekim.setEnabled(false);
        btn_ileri.setEnabled(false);
    }
    
    
    
    
    public void randevu(){
        
        
     tarih();
        
          
        
        try{
            conn = db.connect_db(); 
            String sql = "SELECT * FROM HASTANE";
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery(); 
            
            while(rs.next()){
            pnl_randevu.setVisible(true);
            tb_randevu.setSelectedIndex(0);

                    
                cb_il.addItem(rs.getString("HASTANE_IL"));
                
               // cb_hastane.addItem(rs.getString("HASTANE_AD"));
                //cb_poliklinik.addItem(rs.getString("POLIKLINIK_AD"));
              
                int id = cb_il.getSelectedIndex();
                
                
//                if (cb_il.getSelectedIndex()!=0) {
//                
//                      
//                }
                
                
                
          
            }
            
            
        }catch (Exception ex) {
            
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {
                
            }
        }   
    }
    
        
   
    
    private void btn_ileriActionPerformed(java.awt.event.ActionEvent evt) {                                          

        if (cb_il.getSelectedIndex()!=0 && cb_ilce.getSelectedIndex() != 0 && cb_hastane.getSelectedIndex() !=0 && cb_poliklinik.getSelectedIndex() != 0 && cb_hekim.getSelectedIndex() != 0 ) {
          tb_randevu.setSelectedIndex(1);
          h_tarih =  gun + "." + ay + "." + yıl ;
          lbl_d_adsoyad.setText((String) cb_hekim.getSelectedItem());
            
        }
        else{
          JOptionPane.showMessageDialog(null, "Lütfen bilgileri tam giriniz");
        }
        
    }                                         

    String h_il ;
    public void id_bulma(){
           int id = cb_il.getSelectedIndex();
          
           h_il = (String) cb_il.getSelectedItem();
           if (cb_il.getSelectedIndex()!=0) {
            
     
                   
       cb_ilce.removeAllItems();
       cb_ilce.addItem("SEÇİNİZ");
       cb_hastane.removeAllItems();
       cb_hastane.addItem("SEÇİNİZ");
       cb_poliklinik.removeAllItems();
       cb_poliklinik.addItem("SEÇİNİZ");
       cb_hekim.removeAllItems();
       cb_hekim.addItem("SEÇİNİZ");
       
       cb_ilce.setEnabled(false);
       cb_hastane.setEnabled(false);
       cb_poliklinik.setEnabled(false);
       cb_hekim.setEnabled(false);
        btn_ileri.setEnabled(false);
       
        try{

            conn = db.connect_db() ;
            String sql = "SELECT * FROM HASTANE WHERE HASTANE_ID = '" + id + "'";
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery();

           if(rs.next()){
                cb_ilce.addItem(rs.getString("HASTANE_ILCE"));
                cb_hastane.addItem(rs.getString("HASTANE_AD"));
            }

        }catch (Exception ex) {
            JOptionPane.showMessageDialog(null,"hataaaa");
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {

            }
        }
        }
           else{
               JOptionPane.showMessageDialog(null,"Lütfen il Seçiniz");
                cb_ilce.setEnabled(false);
                
                
                
           }
           
    }
    
    private void cb_ilActionPerformed(java.awt.event.ActionEvent evt) {                                      

        panelkapa();
           id_bulma();
           cb_hastane.setEnabled(false);
           cb_poliklinik.setEnabled(false);
           cb_hekim.setEnabled(false);
           cb_ilce.setEnabled(true);
           
        
    }                                     

    
    String saat ; 
   
    public void saat(javax.swing.JButton btn){
        saat = btn.getText();
        btn.setForeground(Color.red);
        btn.setEnabled(false);
        
    }
    public void saatkilitleme(javax.swing.JButton btn1, javax.swing.JButton btn2 , javax.swing.JButton btn3 , javax.swing.JButton btn4 , javax.swing.JButton btn5, javax.swing.JButton btn6){
        
        btn1.setBackground(new java.awt.Color(51, 153, 0));
        btn2.setBackground(new java.awt.Color(51, 153, 0));
        btn3.setBackground(new java.awt.Color(51, 153, 0));
        btn4.setBackground(new java.awt.Color(51, 153, 0));
        btn5.setBackground(new java.awt.Color(51, 153, 0));
        btn6.setBackground(new java.awt.Color(51, 153, 0));
        
        btn1.setForeground(Color.white);
        btn2.setForeground(Color.white);
        btn3.setForeground(Color.white);
        btn4.setForeground(Color.white);
        btn5.setForeground(Color.white);
        btn6.setForeground(Color.white);
        
        btn1.setEnabled(true);
        btn2.setEnabled(true);
        btn3.setEnabled(true);
        btn4.setEnabled(true);
        btn5.setEnabled(true);
        btn6.setEnabled(true);
        
    }
    
    public void saatbloklama(boolean btn[] , javax.swing.JButton btnsec ,javax.swing.JButton btn1, javax.swing.JButton btn2 , javax.swing.JButton btn3 , javax.swing.JButton btn4 , javax.swing.JButton btn5, javax.swing.JButton btn6){
        
        System.out.println("saatbloklama hata");
         for (int i = 0; i < btnboolean.length; i++) {
            
             if (i==0) {
                 if (btnboolean[i]==false) {
                     
                      btn1.setBackground(new java.awt.Color(51, 153, 0));
                      btn1.setForeground(Color.white);
                      btn1.setEnabled(true);

                 }
             }
             
             else if (i==1) {
                 if (btnboolean[i] == false) {
                     
                      btn2.setBackground(new java.awt.Color(51, 153, 0));
                      btn2.setForeground(Color.white);
                      btn2.setEnabled(true);
                      
                 }
             }
             
             else if (i==2) {
                 if (btnboolean[i] == false) {
                     
                      btn3.setBackground(new java.awt.Color(51, 153, 0));
                      btn3.setForeground(Color.white);
                      btn3.setEnabled(true);
                     
                 }
             }
             
             else if (i==3) {
                 if (btnboolean[i]==false) {
                     
                      btn4.setBackground(new java.awt.Color(51, 153, 0));
                      btn4.setForeground(Color.white);
                      btn4.setEnabled(true);
                     
                 }
             }
             
             else if (i==4) {
                 if (btnboolean[i] == false) {
                     
                      btn5.setBackground(new java.awt.Color(51, 153, 0));
                      btn5.setForeground(Color.white);
                      btn5.setEnabled(true);
                     
                 }
             }
             
             else if (i==5) {
                 if (btnboolean[i] == false) {
                     
                      btn6.setBackground(new java.awt.Color(51, 153, 0));
                      btn6.setForeground(Color.white);
                      btn6.setEnabled(true);
                     
                 }

             }
             
        }
        
      
        
        saat(btnsec);
        
        
    }

    
    void panel (javax.swing.JPanel pnl,javax.swing.JPanel pnl1,javax.swing.JPanel pnl2,javax.swing.JPanel pnl3,javax.swing.JPanel pnl4,javax.swing.JPanel pnl5,javax.swing.JPanel pnl6){
           pnl.setVisible(true);
           pnl1.setVisible(false);
           pnl2.setVisible(false);
           pnl3.setVisible(false);
           pnl4.setVisible(false);
           pnl5.setVisible(false);
           pnl6.setVisible(false);
           



       }
    
    
    
    int doktor_id ; 
    int poliklinik_id ;
    
    
    String doktor_ad ;
    String doktor_soyad ;
    
    void hekim(){
    
         int id = cb_poliklinik.getSelectedIndex();
        
       h_poliklinik =(String) cb_poliklinik.getSelectedItem();
        if (id!=0) {
            
         cb_hekim.setEnabled(true);
         cb_hekim.removeAllItems();
         cb_hekim.addItem("SEÇİNİZ");
         
            try{
              conn = db.connect_db(); 
              String sql = "SELECT * FROM DOKTOR WHERE DOKTOR_ID = '" + id + "'"; 
              pst = conn.prepareStatement(sql);
              rs = pst.executeQuery() ; 
            //  JOptionPane.showMessageDialog(null, "başarı");
              while(rs.next()){
                  d_id = id ;          
                  String ad_soyad = rs.getString("DOKTOR_AD") +  " " + rs.getString("DOKTOR_SOYAD");
                  cb_hekim.addItem(ad_soyad);
                  
                 doktor_ad = rs.getString("DOKTOR_AD");
                 doktor_soyad = rs.getString("DOKTOR_SOYAD");
                  
              }
              
            }catch (Exception ex) {
              JOptionPane.showMessageDialog(null, ex);
                
            }finally{
                try{
                    conn.close();
                }catch (Exception ex) {
                    
                }
            }
        }
        
    }
    
    private void cb_hekimActionPerformed(java.awt.event.ActionEvent evt) {                                         
    panelkapa();
        if (cb_hekim.getSelectedIndex()!=0) {
            btn_ileri.setEnabled(true);
        }
        
    }                                        

    
    int d_id ; 
    String h_poliklinik ;    
    private void cb_poliklinikActionPerformed(java.awt.event.ActionEvent evt) {                                              
        panelkapa();
        
        hekim();
    
        btn_ileri.setEnabled(false);

        
        
    }                                             

    private void cb_hastaneActionPerformed(java.awt.event.ActionEvent evt) {                                           
            panelkapa();
            if (cb_hastane.getSelectedIndex()!=0) {
            cb_poliklinik.setEnabled(true);
            cb_poliklinik.removeAllItems(); 
            cb_poliklinik.addItem("SEÇİNİZ");
            
            cb_hekim.setEnabled(false);
            btn_ileri.setEnabled(false);
            
            try{
                
                conn = db.connect_db() ; 
                
                String sql = "SELECT * FROM POLIKLINIK" ; 
                pst = conn.prepareStatement(sql);
                rs = pst.executeQuery() ; 
                
                while(rs.next()){
                    cb_poliklinik.addItem(rs.getString("POLIKLINIK_AD"));
                }
                
            }catch (Exception ex) {
                    JOptionPane.showMessageDialog(null, ex);
              }finally{
                try{
                  conn.close();
                } catch (Exception ex) {
                    
                }  
            }
        }
       

    }                                          

    public void randevukayıt(){
        
            System.out.println("randevukayıt hata");
          try{
           conn=db.connect_db();
           String sql="INSERT INTO RANDEVU_ISLEM (RANDEVU_IL,RANDEVU_ILCE,HASTANE_AD,RANDEVU_POLIKLINIK,RANDEVU_TARIH,DOKTOR_ID,HASTA_ID,RANDEVU_SAAT) VALUES (?,?,?,?,?,?,?,?) ";
           pst=conn.prepareStatement(sql);
           pst.setString(1,(String) cb_il.getSelectedItem());
           pst.setString(2,(String) cb_ilce.getSelectedItem());
           pst.setString(3,(String) cb_hastane.getSelectedItem());
           pst.setString(4,(String) cb_poliklinik.getSelectedItem());
           pst.setString(5,(String) h_tarih);
           pst.setString(6,""+ d_id + "");
           pst.setString(7,""+ h_id + "");
           pst.setString(8,""+ saat + "");

            pst.execute();
            JOptionPane.showMessageDialog(null,"Randevu Onaylandı");
            new Ana_Ekran(h_sql).setVisible(true);
            this.hide();

             


       }catch (Exception ex) {
            JOptionPane.showMessageDialog(null, ex);

        }
       finally {
           try{
               conn.close();
           }

           catch (Exception ex) {
               JOptionPane.showMessageDialog(null, ex);
           }
       }
        
                
    }
    
    
    public void btnsaatleri(boolean btnboolean[]  ,javax.swing.JButton btn1, javax.swing.JButton btn2 , javax.swing.JButton btn3 , javax.swing.JButton btn4 , javax.swing.JButton btn5, javax.swing.JButton btn6){
      
//        panel(pnl_9,pnl_10,pnl_11,pnl_12,pnl_13,pnl_14,pnl_15);
        
        System.out.println("btnsaatleri hata");
        for (int i = 0; i <btnboolean.length; i++) {
            btnboolean[i] = false ;
        }
        
        try{
            
            conn = db.connect_db() ; 
            String sql = "SELECT * FROM RANDEVU_ISLEM" ; 
            pst = conn.prepareStatement(sql);
            rs = pst.executeQuery();
                     saatkilitleme(btn1 , btn2 , btn3 , btn4 , btn5 , btn6);
            
            while(rs.next()){
                String rtarih = rs.getString("RANDEVU_TARIH");
                String rsaat = rs.getString("RANDEVU_SAAT");
                String rpoliklinik = rs.getString("RANDEVU_POLİKLİNİK");
                //String ril = rs.getString("RANDEVU_IL");
                //System.out.println(ril);
                System.out.println(rpoliklinik);
                System.out.println(h_poliklinik);
                System.out.println(rtarih);
                System.out.println(h_tarih);
                System.out.println(rsaat);
               // System.out.println(h_saat);
                System.out.println("\n \n");
                
                if(rtarih.equals(h_tarih)) {
                    
                String btnsaat[] = new String[6]; 
                       
                     
                     
                     
                    
                 btnsaat[0] = btn1.getText();
                 btnsaat[1] = btn2.getText();
                 btnsaat[2] = btn3.getText();
                 btnsaat[3]= btn4.getText();
                 btnsaat[4] = btn5.getText();
                 btnsaat[5] = btn6.getText();
                 
                 
               
                 
                 
                
                 for (int i = 0; i < btnsaat.length; i++) {
                        if (i==0) {
                            
                         if (btnsaat[i].equals(rsaat) && rpoliklinik.equals(h_poliklinik) ) {
                          btn1.setForeground(Color.red);
                          btn1.setEnabled(false);
                          btnboolean[i]= true; 
                         }
                         
                     }
                     
                     else if (i==1) {
                         
                            if (btnsaat[i].equals(rsaat) && rpoliklinik.equals(h_poliklinik) ) {
                                btn2.setForeground(Color.red);
                                btn2.setEnabled(false);
                                btnboolean[i]= true; 

                            }
                            
                     }
                     
                     else if (i==2) {
                         
                          if (btnsaat[i].equals(rsaat) && rpoliklinik.equals(h_poliklinik)) {
                                 btn3.setForeground(Color.red);
                                 btn3.setEnabled(false);
                                 btnboolean[i]= true; 

                         }

                         
                     }
                     
                     else if (i==3) {
                         
                          if (btnsaat[i].equals(rsaat) && rpoliklinik.equals(h_poliklinik) ) {
                              btn4.setForeground(Color.red);
                              btn4.setEnabled(false);
                              btnboolean[i]= true; 
                         }

                         
                     }
                     
                     else if (i==4) {
                         
                          if (btnsaat[i].equals(rsaat) && rpoliklinik.equals(h_poliklinik)) {
                              btn5.setForeground(Color.red);
                              btn5.setEnabled(false);
                              btnboolean[i]= true; 
                         }

                         
                     }
                     
                     else if (i==5) {
                         
                          if (btnsaat[i].equals(rsaat) && rpoliklinik.equals(h_poliklinik)) {
                              btn6.setForeground(Color.red);
                              btn6.setEnabled(false);
                              btnboolean[i]= true; 
                         }

                         
                     }
                }
                 
                }
System.out.println("döngü");
                 
           }
            
        }catch (Exception ex) {
            JOptionPane.showMessageDialog(null, ex);
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {
                
            }
        }
        
    }
    
    

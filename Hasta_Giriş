    String h_sql ; 
    
    
     public Hasta_Giris(String sql) {
         initComponents();
       h_sql = sql;
          
          
          isim(sql);
         
          randevu();

          cb_kilitleme();
          panelkapa();
        Toolkit toolkit=getToolkit();
        Dimension size=toolkit.getScreenSize();
        setLocation(size.width/2-getWidth()/2,size.height/2-getHeight()/2);
        
    }
     
     
     void panelkapa (){
          
           pnl_9.setVisible(false);
           pnl_10.setVisible(false);
           pnl_11.setVisible(false);
           pnl_12.setVisible(false);
           pnl_13.setVisible(false);
           pnl_14.setVisible(false);
           pnl_15.setVisible(false);

     
     }

     
     

     
     
    int h_id ;
     public void isim(String sql){
        
         
         try{
             conn = db.connect_db(); 
            // String sql  = "SELECT * FROM HASTA WHERE HASTA_ID = '" + a + "'" ;
             pst = conn.prepareStatement(sql);
             rs = pst.executeQuery() ; 
             System.out.println(sql);
           
             while(rs.next()){
                 h_id = Integer.parseInt(rs.getString("HASTA_ID"));
             }
             
         }catch (Exception ex) {
             JOptionPane.showMessageDialog(null,ex);
         }finally{
             try{
                 conn.close();
             }catch (Exception ex) {
                 
             }
         }
         
         
     }
    
    
    
    

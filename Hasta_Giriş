    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         

        
        String  idd = Integer.toString(doktor_id);
       String  h_idd = Integer.toString(h_id);
        try{
            conn = db.connect_db() ; 
            String sql = "INSERT INTO FAVORI (DOKTOR_AD , DOKTOR_SOYAD , FAVORI_POLIKLINIK , HASTA_ID ) VALUES (?,?,?,?) " ;
            pst = conn.prepareStatement(sql);

            pst.setString(1, doktor_ad );
            pst.setString(2,doktor_soyad);
            pst.setString(3,(String)  cb_poliklinik.getSelectedItem() );
            pst.setString(4,"" + h_id +  "");
            pst.execute() ; 
            
            String adsoyad = doktor_ad + " " + doktor_soyad;
            JOptionPane.showMessageDialog(null , adsoyad +" favorilere eklendi");



        }catch (Exception ex) {
            JOptionPane.showMessageDialog(null, ex);
        }finally{
            try{
                conn.close();
            }catch (Exception ex) {

            }
        }
        
        
        
        
        
        
        
